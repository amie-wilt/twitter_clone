<% @users.each do |user| %>
  <ul>
    <li>
      <%= user.name %>
    </li>
    <li>
      <ul>
        <% user.moos.each do |moo| %>
          <li>
            <%= moo.description %>
            created at <%= moo.created_at %>
          </li>
        <% end %>
      </ul>
    </li>
    <li>
      <% if current_user.following?(user) && current_user != user %>
        <%= link_to "Unfollow", unfollow_user_path(user) %>
      <% else %>
        <%= link_to "Follow", follow_user_path(user)%>
      <% end%>
    </li>
  </ul>


<% end %>

