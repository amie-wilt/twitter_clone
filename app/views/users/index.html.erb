<% @users.each do |user| %>
  <ul>
    <li>
      <%= user.name %>
    </li>
    <li>
      <ul>
        <% user.moos.each do |moo| %>
          <li>
            <%= moo.description %>
            created at <%= moo.created_at %>
          </li>
        <% end %>
      </ul>
    </li>
    <li>
      <%= link_to 'Follow', follow_user_path(user) %>
    </li>
    <li>
      <%= link_to 'Unfollow', unfollow_user_path(user) %>
    </li>
  </ul>


<% end %>